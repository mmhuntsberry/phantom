{
  "name": "phantom",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "storybook": "npx nx run phantom-components:storybook",
    "build-storybook": "npx nx run phantom-components:build-storybook",
    "test-storybook": "npx test-storybook --url http://localhost:4400/ --coverage --config-dir packages/phantom-components/.storybook",
    "chromatic": "npx chromatic",
    "dev:portfolio": "npx nx serve portfolio --dev",
    "dev:author": "npx nx serve author --dev",
    "dupe": "git archive -o phantom.zip HEAD",
    "author:db:create-enums": "cd apps/author && dotenv -e .env.local -- ts-node scripts/create-enums.ts",
    "author:db:ensure-index": "cd apps/author && dotenv -e .env.local -- ts-node scripts/ensure-index.ts",
    "author:db:fix-constraints": "cd apps/author && dotenv -e .env.local -- ts-node scripts/fix-constraints.ts",
    "author:db:reset-constraints": "cd apps/author && dotenv -e .env.local -- ts-node scripts/reset-constraints.ts",
    "author:db:generate": "cd apps/author && dotenv -e .env.local -- drizzle-kit generate",
    "author:db:migrate": "cd apps/author && dotenv -e .env.local -- ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" scripts/run-migrations.ts",
    "author:db:push": "cd apps/author && dotenv -e .env.local -- drizzle-kit push",
    "author:admin:create-user": "cd apps/author && dotenv -e .env.local -- ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" scripts/create-admin-user.ts",
    "author:clean:beta-readers": "cd apps/author && dotenv -e .env.local -- ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" scripts/clean-beta-readers.ts",
    "author:test:e2e": "PLAYWRIGHT_BASE_URL=http://localhost:4200 dotenv -e apps/author/.env.local -- playwright test -c apps/author/playwright.config.ts",
    "author:test:e2e:serve": "PLAYWRIGHT_START_SERVER=1 dotenv -e apps/author/.env.local -- playwright test -c apps/author/playwright.config.ts",
    "deploy:author": "bash scripts/deploy-author.sh",
    "deploy:portfolio": "bash scripts/deploy-portfolio.sh"
  },
  "private": true,
